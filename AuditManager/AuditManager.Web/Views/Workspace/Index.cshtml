@{
    Layout = "~/Views/Shared/Layout/_Layout.cshtml";
}

<div class="row-fluid col-xs-12 col-md-12 padding-0">
    <div id="mainTab" class="mainTab-dimension">
        <ul>
            <li><a href="#wsEng">My Engagements</a></li>
            <li><a href="#wsAct">RET and RF Status</a></li>
            <li><a href="#wsGuide">Guidance</a></li>
            <li><a href="#wsCreate">Create Workspace</a></li>

            @if (AuditManager.Common.AmUtil.IsSuperUser())
            {
                <li><a href="#wsAdmin">Admin</a></li>
            }
        </ul>
        <div id="wsEng">
            <div class="row-fluid col-xs-12 col-md-12 padding-0">
                <div class="col-xs-3 col-md-3 col-lg-3 padding-0">
                    @Html.Partial("MyEng/_LeftPartial")
                </div>
                <div class="col-xs-9 col-md-9 col-lg-9 padding-0">
                    @Html.Partial("MyEng/_RightPartial")
                </div>
            </div>
        </div>
        <div class="row-fluid col-xs-12 col-md-12 padding-0 content-only">
            <div id="wsAct">
                @Html.Partial("OuterTab/_ActivityPartial")
            </div>
            <div id="wsGuide">
                @Html.Partial("OuterTab/_GuidePartial")
            </div>
            <div id="wsCreate">
                @Html.Partial("OuterTab/_CreatePartial")
            </div>
            @if (AuditManager.Common.AmUtil.IsSuperUser())
            {
                <div id="wsAdmin">
                    @Html.Partial("OuterTab/_AdminPartial")
                </div>
            }
        </div>
    </div>
</div>
